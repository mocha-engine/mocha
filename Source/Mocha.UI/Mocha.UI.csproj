	<ItemGroup>
		<PackageReference Include="FlexLayoutSharp" Version="1.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Mocha.Common\Mocha.Common.csproj" />
	</ItemGroup>

</Project>
